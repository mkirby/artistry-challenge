<% if flash[:errors] %>
    <%= flash[:errors].each do |error| %>
        <%= error %>
    <% end %>
<% end %>

<%= form_for @artist do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :age %><br>
    <%= f.text_field :age %><br>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
    <%= f.submit %><br><br>
<% end%>